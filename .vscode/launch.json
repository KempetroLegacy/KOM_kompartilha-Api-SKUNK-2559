{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "NestJS CLI Debug",
      "program": "${workspaceFolder}/node_modules/@nestjs/cli/bin/nest.js",
      "args": ["start", "--watch"],
      "autoAttachChildProcesses": true,
      "console": "integratedTerminal"
    }
  ]
}